Algoritmo ESN13
	Definir numeroUno Como Entero;
	Definir numeroDos Como Entero;
	
	Mostrar "Ingrese el primer numero: ";
	Leer numeroUno;
	Mostrar "Ingrese el segundo numero: ";
	Leer numeroDos;
	
	Si numeroUno <999 o numeroDos >999 Entonces
		Mostrar "El numero ",numeroUno " es menor";
		Mostrar "El numero ",numeroDos " es mayor";
	FinSi
	
	
	
	
	
	
	
	
	
FinAlgoritmo
