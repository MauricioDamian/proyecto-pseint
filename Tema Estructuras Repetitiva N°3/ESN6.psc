Algoritmo ESN6
	
	Definir unNumero Como Entero;
	Definir multiplicacion Como Real;
	
	Mostrar "Ingrese un numero: ";
	Leer unNumero;
	
	i = 0
	
	Mientras i < 10 Hacer
		
		i = i + 1;
		
		resultado = unNumero * i
		
		
		
		
		Mostrar " ",unNumero, "*", i  " = ", resultado, "";
		
		
	FinMientras
	
	Mostrar "Fin del programa"
	
FinAlgoritmo
